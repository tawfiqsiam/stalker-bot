{
  "name": "stalker-bot",
  "version": "1.0.13",
  "description": "Custom Halo/Warframe/League of legends/Server Discord bot",
  "main": "bot.js",
  "scripts": {
    "test": "npm run lint",
    "lint": "eslint --debug bot.js",
    "wincheck": "node ./test/OsCheck/WinPreInstall.js",
    "newcmd": "bash assets/tools/build-command.sh",
    "wininstall": "npm i --global --production --save windows-build-tools ffmpeg-binaries",
    "start": "pm2 start bot.js",
    "watch": "pm2 start bot.js  --watch ",
    "stop": "pm2 stop bot",
    "restart": "pm2 restart bot",
    "showlogs": "pm2 logs bot",
    "clearlogs": "pm2 flush bot"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Co-Op-Development/stalker-bot.git"
  },
  "keywords": [
    "discord",
    "discord-js",
    "discord-bot"
  ],
  "author": "Ittim, D4RKAR117, Coop-Development",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/Co-Op-Development/stalker-bot/issues"
  },
  "homepage": "https://github.com/Co-Op-Development/stalker-bot#readme",
  "dependencies": {
    "babel-eslint": "^10.0.1",
    "canvas": "^2.2.0",
    "discord.js": "^11.4.2",
    "dotenv": "^6.2.0",
    "eslint": "^5.10.0",
    "eslint-config-defaults": "^9.0.0",
    "is-playlist": "^1.0.0",
    "lodash": "^4.17.11",
    "moment": "^2.23.0",
    "node-opus": "^0.3.1",
    "pm2": "^3.2.3",
    "simple-youtube-api": "^5.1.1",
    "ytdl-core": "^0.26.3"
  }
}
