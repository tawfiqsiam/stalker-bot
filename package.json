{
  "name": "stalker-bot",
  "version": "1.0.10",
  "description": "Custom Halo/Warframe/League of legends/Server Discord bot",
  "main": "bot.js",
  "scripts": {
    "test": "npm run lint",
    "lint": "eslint --debug bot.js",
    "wincheck": "node ./test/OsCheck/WinPreInstall.js",
    "windows-install": "npm i --global --production --save windows-build-tools ffmpeg-binaries",
    "start": "pm2 start bot.js",
    "launch-dev": "pm2 start bot.js  --watch ",
    "stop": "pm2 stop bot",
    "restart": "pm2 restart bot",
    "show-logs": "pm2 logs bot",
    "clear-logs": "pm2 flush bot"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/D4RKAR117/stalker-bot.git"
  },
  "keywords": [
    "discord",
    "discord-js",
    "discord-bot"
  ],
  "author": "D4RKAR117",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/D4RKAR117/stalker-bot/issues"
  },
  "homepage": "https://github.com/D4RKAR117/stalker-bot#readme",
  "dependencies": {
    "babel-eslint": "^10.0.1",
    "canvas": "^2.0.1",
    "discord.js": "^11.4.2",
    "dotenv": "^6.1.0",
    "eslint": "^5.6.1",
    "eslint-config-defaults": "^9.0.0",
    "is-playlist": "^1.0.0",
    "node-opus": "^0.3.0",
    "pm2": "^3.2.2",
    "simple-youtube-api": "^5.0.2",
    "ytdl-core": "^0.26.3"
  }
}
